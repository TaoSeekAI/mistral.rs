# Mistral.rs Docker Configuration
# Copy this file to .env and configure your settings

# GitHub repository for image tags (format: owner/repo)
GITHUB_REPOSITORY=mistralrs/mistralrs

# Port configuration
MISTRALRS_PORT=1234
DEBUG_PORT=5678

# GPU configuration
CUDA_COMPUTE_CAP=89
NVIDIA_VISIBLE_DEVICES=all

# Model configuration
HF_TOKEN=your_huggingface_token_here

# Logging
RUST_LOG=info

# Docker registry settings (for CI/CD)
REGISTRY=ghcr.io
IMAGE_NAME=mistralrs/mistralrs